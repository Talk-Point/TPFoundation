  "type": "lib",
  "require": {
    "php": ">=5.5.9",
    "tedivm/stash": "^0.13.1",
    "pda/pheanstalk": "^3.0",
    "predis/predis": "^1.0",
    "ramsey/uuid": "stable",
    "monolog/monolog": "^1.17"
  },
  "require-dev": {
